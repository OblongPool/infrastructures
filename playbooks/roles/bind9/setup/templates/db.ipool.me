;
; BIND data file for ipool.me
;
$TTL    604800
@       IN      SOA     localhost. root.localhost. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
{{ VAULT_FQDN }}. IN      A       {{ hostvars.vault.ansible_host if hostvars.vault is defined else "127.0.0.1" }}
